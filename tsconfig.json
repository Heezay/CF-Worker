{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      // "types": ["@cloudflare/workers-types"]
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }